{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=GestorEstudiantilDb;Trusted_Connection=True;"
  },
  "Settings": {
    "Estudiante": {
      "Edad": 18,
      "Nombre": "Estudiante",
